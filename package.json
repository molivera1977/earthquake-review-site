{
  "name": "earthquakes-review-site",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "deploy": "gh-pages -d dist -b gh-pages",
    "publish": "git add -A && read -p 'Commit message: ' msg && git commit -m \"$msg\" && git push origin main && npm run build && npm run deploy"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "gh-pages": "^6.1.1",
    "vite": "^5.4.8"
  }
}
